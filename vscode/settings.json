{
  "editor.semanticHighlighting.enabled": true,
  "github.copilot.editor.enableAutoCompletions": false,
  "update.mode": "default",
  "editor.bracketPairColorization.enabled": false,
  "markdown-preview-github-styles.colorTheme": "light",
  "search.exclude": {
    "node_modules": true
  },
  "editor.lineNumbers": "relative",
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "editor.fontFamily": "Hack",
  "editor.fontLigatures": false,
  "files.trimTrailingWhitespace": true,
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "solid",
  "editor.tabSize": 2,
  "editor.roundedSelection": false,
  "editor.rulers": [100],
  "editor.parameterHints.enabled": false,
  "explorer.openEditors.visible": 0,
  "editor.hideCursorInOverviewRuler": true,
  "editor.tabCompletion": "on",
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "trailing",
  "debug.enableStatusBarColor": false,
  "vscode-neovim.compositeKeys": {
    "jk": {
      "command": "vscode-neovim.escape"
    }
  },
  "explorer.confirmDelete": false,
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "[graphql]": {
    "editor.formatOnSave": true
  },
  "[markdown]": {
    "rewrap.autoWrap.enabled": true,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.insertSpaces": true
  },
  "[plaintext]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.insertSpaces": true,
    "editor.wordWrap": "on",
    "editor.wordWrapColumn": 80
  },
  "[yaml]": {
    "editor.formatOnSave": false,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.insertSpaces": true,
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "editor.tokenColorCustomizations": {
    "[Cobalt2]": {
      "textMateRules": [
        {
          "scope": "keyword.interface",
          "settings": {
            "foreground": "#80FFBB"
          }
        },
        {
          "scope": "keyword.struct",
          "settings": {
            "foreground": "#80FFBB"
          }
        },
        {
          "scope": "keyword.map",
          "settings": {
            "foreground": "#80FFBB"
          }
        },
        {
          "scope": "storage.type",
          "settings": {
            "foreground": "#80FFBB"
          }
        },
        {
          "scope": "entity.name.import.go",
          "settings": {
            "foreground": "#A5FF90"
          }
        },
        {
          "scope": "support.function.go",
          "settings": {
            "foreground": "#FFC600"
          }
        }
      ]
    }
  },
  "workbench.colorCustomizations": {
    "[Cobalt2]": {
      "editorCursor.foreground": "#ffc50099",
      "editorCursor.background": "#deefff",
      "tab.activeBorder": "#193549",
      "tab.activeBackground": "#193549",
      "editorGroupHeader.tabsBackground": "#152d3e",
      "tab.inactiveBackground": "#152733",
      "sideBar.background": "#152d3e",
      "sideBar.foreground": "#99abb8",
      "sideBar.border": "#13232e",
      "editor.lineHighlightBorder": "#1F4662",
      "errorForeground": "#ff776b",
      "editorLineNumber.activeForeground": "#456eab",
      "editorGutter.background": "#193549",
      "editorGutter.deletedBackground": "#86535b",
      "editorGutter.addedBackground": "#6fae5a",
      "editorGutter.modifiedBackground": "#97885a",
      "gitDecoration.modifiedResourceForeground": "#e2c18d",
      "gitDecoration.deletedResourceForeground": "#747476",
      "gitDecoration.untrackedResourceForeground": "#6fc38d",
      "gitDecoration.ignoredResourceForeground": "#747476",
      "gitDecoration.conflictingResourceForeground": "#FF7200",
      "scrollbarSlider.background": "#ffffff25",
      "scrollbarSlider.hoverBackground": "#ffffff45",
      "scrollbarSlider.activeBackground": "#ffffff45",
      "statusBar.border": "#13232e",
      "statusBar.debuggingBorder": "#13232e",
      "statusBar.debuggingForeground": "#aaaaaa",
      "pickerGroup.foreground": "#99abb8",
      "input.border": "#0f202c",
      "input.background": "#102432",
      "panel.border": "#0f202c",
      "peekView.border": "#0f202c",
      "peekViewTitle.background": "#152d3e",
      "peekViewResult.background": "#152d3e",
      "peekViewResult.selectionBackground": "#193549",
      "peekViewResult.matchHighlightBackground": "#193549",
      "peekViewEditorGutter.background": "#152d3e"
    }
  },
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.cursorStyle": "line",
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.decorations.colors": false,
  "telemetry.telemetryLevel": "error",
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.editor.tabSizing": "fit",
  "editor.fontSize": 15,
  "explorer.decorations.badges": false,
  "search.smartCase": true,
  "editor.minimap.enabled": false,
  "files.insertFinalNewline": true,
  "workbench.enableExperiments": false,
  "breadcrumbs.enabled": false,
  "window.restoreWindows": "all",
  "go.useLanguageServer": true,
  "editor.formatOnSave": true,
  "[go]": {
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    },
    "editor.insertSpaces": false,
    "editor.detectIndentation": false
  },
  "files.eol": "\n",
  "go.testFlags": ["-v", "-count", "1"],
  "go.testEnvFile": "${workspaceFolder}/environment/experimental",
  "editor.matchBrackets": "never",
  "workbench.iconTheme": "file-icons",
  "explorer.autoReveal": false,
  "problems.autoReveal": false,
  "debug.console.fontSize": 15,
  "extensions.ignoreRecommendations": true,
  "editor.snippetSuggestions": "top",
  "gopls": {
    // Experimental settings
    "completeUnimported": true, // autocomplete unimported packages
    "usePlaceholders": true,
    "deepCompletion": true, // enable deep completion
    "analyses": {
      "fillreturns": true,
      "undeclaredname": true,
      "unusedparams": true,
      "nonewvars": true
    }
  },
  "files.associations": {
    "*.Jenkinsfile": "groovy",
    "*.txt": "markdown"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "window.newWindowDimensions": "inherit",
  "workbench.editor.showTabs": "multiple",
  "editor.suggestSelection": "first",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "go.toolsManagement.autoUpdate": true,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "redhat.telemetry.enabled": false,
  "[ruby]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "LoranKloeze.ruby-rubocop-revived"
  },
  "rubyLsp.enabledFeatures": {
    "codeActions": true,
    "diagnostics": true,
    "documentHighlights": true,
    "documentLink": true,
    "documentSymbols": true,
    "foldingRanges": true,
    "formatting": false,
    "hover": true,
    "inlayHint": true,
    "onTypeFormatting": true,
    "selectionRanges": true,
    "semanticHighlighting": false,
    "completion": true,
    "codeLens": false
  },
  "openInGitHub.useLocalLine": true,
  "workbench.preferredLightColorTheme": "GitHub Light Default",
  "workbench.colorTheme": "GitHub Light",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.openRepositoryInParentFolders": "always",
  "cSpell.enabledLanguageIds": ["markdown"],
  "[javascript][typescript][typescriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": "explicit"
    }
  },
  "svelte.enable-ts-plugin": true,
  "debug.onTaskErrors": "debugAnyway",
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "window.confirmSaveUntitledWorkspace": false,
  "diffEditor.renderSideBySide": false,
  "[python]": {
    "diffEditor.ignoreTrimWhitespace": false
  },
  "workbench.editor.enablePreview": false,
  "diffEditor.codeLens": true,
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "workbench.activityBar.location": "hidden",
  "cSpell.userWords": ["postamble", "UNSTAGED"],
  "diffEditor.hideUnchangedRegions.enabled": true,
  "chat.commandCenter.enabled": false,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  }
}
