{
  "editor.fontFamily": "SFMono Nerd Font",
  "editor.lineNumbers": "relative",
  "files.trimTrailingWhitespace": true,
  "editor.fontWeight": "400",
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "solid",
  "editor.tabSize": 2,
  "editor.roundedSelection": false,
  "editor.rulers": [
    80
  ],
  "search.location": "panel",
  "explorer.openEditors.visible": 0,
  "editor.hideCursorInOverviewRuler": true,
  "editor.tabCompletion": "on",
  "editor.renderLineHighlight": "all",
  "vim.useSystemClipboard": true,
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.duration": 100,
  "vim.highlightedyank.color": "#004FAA",
  "vim.insertModeKeyBindings": [
    {
      "before": [
        "j",
        "k"
      ],
      "after": [
        "<Esc>"
      ]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": [
        "g",
        "p"
      ],
      "commands": [
        "editor.action.peekDefinition"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.referenceSearch.trigger"
      ]
    },
    {
      "before": [
        "g",
        "t"
      ],
      "commands": [
        "go.toggle.test.file"
      ]
    },
    {
      "before": [
        "t",
        "c"
      ],
      "commands": [
        "go.test.cursor"
      ]
    },
    {
      "before": [
        "t",
        "t"
      ],
      "commands": [
        "go.test.file"
      ]
    },
    {
      "before": [
        "t",
        "p"
      ],
      "commands": [
        "go.test.package"
      ]
    }
  ],
  "vim.visualModeKeyBindings": [],
  "vim.statusBarColorControl": false,
  "vim.statusBarColors.normal": [
    "#69ee66",
    "#434C5E"
  ],
  "vim.statusBarColors.insert": [
    "#0cc1f3",
    "#FFFFFF"
  ],
  "vim.statusBarColors.visual": [
    "#ffce62",
    "#aa770a"
  ],
  "vim.statusBarColors.visualline": "#B48EAD",
  "vim.statusBarColors.visualblock": "#A3BE8C",
  "vim.statusBarColors.replace": "#D08770",
  "explorer.confirmDelete": false,
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.formatOnSave": true
  },
  "[graphql]": {
    "editor.formatOnSave": true
  },
  "workbench.startupEditor": "none",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.colorCustomizations": {
    "[Cobalt2]": {
      "editorCursor.foreground": "#ffc50099",
      "editorCursor.background": "#deefff",
      "tab.activeBorder": "#14354b",
      "tab.activeBackground": "#14354b",
      "editorGroupHeader.tabsBackground": "#152733",
      "tab.inactiveBackground": "#152733",
      "sideBar.background": "#152733",
      "sideBar.foreground": "#99abb8",
      "editor.lineHighlightBorder": "#1F4662",
      "editorLineNumber.activeForeground": "#56729c",
      "editorGutter.background": "#14354b",
      "editorGutter.deletedBackground": "#86535b",
      "editorGutter.addedBackground": "#6fae5a",
      "editorGutter.modifiedBackground": "#97885a",
      "gitDecoration.modifiedResourceForeground": "#e2c18d",
      "gitDecoration.deletedResourceForeground": "#747476",
      "gitDecoration.untrackedResourceForeground": "#6fc38d",
      "gitDecoration.ignoredResourceForeground": "#747476",
      "gitDecoration.conflictingResourceForeground": "#FF7200",
    },
  },
  "atomKeymap.promptV3Features": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.formatOnPaste": true,
  "explorer.confirmDragAndDrop": false,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "workbench.activityBar.visible": false,
  "workbench.tips.enabled": false,
  "workbench.tree.indent": 15,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.editor.tabSizing": "shrink",
  "editor.fontSize": 15,
  "window.zoomLevel": 0.2,
  "explorer.decorations.badges": false,
  "search.smartCase": true,
  "editor.minimap.enabled": false,
  "files.insertFinalNewline": true,
  "workbench.enableExperiments": false,
  "breadcrumbs.enabled": false,
  "window.restoreWindows": "all",
  "go.useLanguageServer": true,
  "editor.formatOnSave": true,
  "[go]": {
    "editor.codeActionsOnSaveTimeout": 3000,
    "editor.snippetSuggestions": "none",
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    }
  },
  "files.eol": "\n",
  "go.testFlags": [
    "-v",
    "-count",
    "1"
  ],
  "go.testEnvFile": "${workspaceFolder}/environment/experimental",
  "go.formatTool": "goformat",
  "editor.matchBrackets": false,
  "workbench.iconTheme": "file-icons",
  "explorer.autoReveal": false,
  "problems.autoReveal": false,
  "workbench.colorTheme": "Cobalt2"
}
