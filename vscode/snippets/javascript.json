{
  "Console log": {
    "prefix": "cl",
    "body": "console.log($1);"
  },
  "then": {
    "prefix": "then",
    "body": "then(() => {\n  ${0}\n});"
  },
  "require": {
    "prefix": "req",
    "body": "const ${1} = require('${2:path}');"
  },
  "module-import": {
    "prefix": "im",
    "body": "import ${1} from ${2};"
  },
  "module-export": {
    "prefix": "ex",
    "body": "export ${0};"
  },
  "module-export-default": {
    "prefix": "ed",
    "body": "export default $0;"
  },
  "mocha-context": {
    "prefix": "con",
    "body": "context('${1}', () => {\n  $0\n});"
  },
  "jasmine-waitsFor": {
    "prefix": "wai",
    "body": "waitsFor(function() {\n  $0\n});"
  },
  "jasmine-it": {
    "prefix": "it",
    "body": "it('${1}', () => {\n  $0\n});"
  },
  "jasmine-describe": {
    "prefix": "des",
    "body": "describe('${1}', () => {\n  $0\n});"
  },
  "jasmine-before": {
    "prefix": "bef",
    "body": "beforeEach(() => {\n  $0\n});"
  },
  "jasmine-after": {
    "prefix": "aft",
    "body": "afterEach(() => {\n  $0\n});"
  }
}
